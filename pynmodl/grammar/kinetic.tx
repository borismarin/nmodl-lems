import comment
import keyword
import variable
import expressions
import state

Kinetic: 'KINETIC' name=ID '{' statements*=KineticStatement '}';
KineticStatement: Reaction |  Conserve | Statement;
Reaction: '~' reactant=[Variable|cID] '<->' product=[Variable|cID] '(' fwd=Expression ',' rev=Expression ')';
Conserve: 'CONSERVE' reactants=React '=' val=Addition;

React: op=Primary ('+' op=Primary)*;
VarRef: var=[Variable|cID];
Primary: INT VarRef | VarRef | INT;

